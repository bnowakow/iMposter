html, body {
	color: #FFF;
	margin: 0;
	padding: 0;
	background: #dcdcdc url(/themes/default/style/images/background.png) repeat-x top left;
}

body
{
	text-align: center;
	font: 0.8em "Segoe UI", "Trebuchet MS", Arial, Verdana, Helvetica, FreeSans, sans-serif;
}

a:link
{
color: #F4BF33;
text-decoration: none;
}

a:hover
{
text-decoration: underline;
}

a img
{
border: none;
}

#container
{
width: 960px;
max-width: 960px;
margin: 0 auto;
}

#header
{
height: 170px;
margin: 0px 0px 18px 0px;
background: url(http://studentguru.gr/blogs/vangos/Images/Blog-Header.jpg) no-repeat top right;
}

#header h2 {
display: block;
float: left;
margin: 0px;
padding: 0px;
width: 442px;
}

#header h2 a
{
display: block;
height: 160px;
color: transparent;
background: url(http://studentguru.gr/blogs/vangos/Images/Blog-Logo.png) no-repeat center center;
}

#tagline
{
display: none;
}

#wrapper {
display: inline;
float: left;
width: 637px;
margin: 0;
}

.entryview, dl.entrylist {
color: #181818;
margin: 0px 5px 15px 0px;
padding: 20px 20px 20px 25px;
background: #fff;
box-shadow: #aaa 1px 1px 5px;
-moz-box-shadow: #aaa 1px 1px 5px;
-webkit-box-shadow: #aaa 1px 1px 5px;
}

.entryview a:link, .entryview a:visited, .entrylist a:link, .entrylist a:visited
{
color: #BA790C;
font-weight: bold;
}

dl.entrylist dt, dl.entrylist dd {
margin: 0;
padding: 0;
font-family: "Segoe UI", "Trebuchet MS", Arial, Verdana, Helvetica, FreeSans, sans-serif;
background: #fff;
}

dl.entrylist dt a:link, dl.entrylist dt a:visited
{
color: #BA790C;
padding: 3px 0px;
}

dl.entrylist dt a:hover
{
color: #fff;
background: #BA790C;
}

.entryviewheading, .contentsubheading {
color: #121212;
font-size: 1.6em;
font-family: "Segoe UI", "Trebuchet MS", Arial, Verdana, Helvetica, FreeSans, sans-serif;
}

.contentsubheading
{
color: #353535;
}

dl.comments dt.owner
{
background-color: #4D4D4D;
}

dl.comments dd.owner
{
background-color: #353535;
}

dl.comments dt
{
background-color: #353535;
font-size: 1.3em;
margin: 0px 0px 0px 20px;
padding: 5px 5px 5px 10px;
}

dl.comments dd
{
background-color: #4D4D4D;
margin: 0px 0px 20px 20px;
overflow: hidden;
padding: 0px 5px 5px 10px;
word-wrap: break-word;
}

dt.owner, dl.comments dt
{
border-top-left-radius: 7px;
-moz-border-top-left-radius: 7px;
-webkit-border-top-left-radius: 7px;
}

dd.owner, dl.comments dd
{
border-bottom-right-radius: 7px;
-moz-border-bottom-right-radius: 7px;
-webkit-border-bottom-right-radius: 7px;
}

.commentform
{
background: #555;
}

dl.commentform
{
margin: 0px 0px 20px 20px;
padding: 10px 20px 20px 20px;
}

dl.commentform dt input
{
color: #181818;
font: 1em "Segoe UI", "Trebuchet MS", Arial, Verdana, Helvetica, FreeSans, sans-serif;
padding: 5px 10px;
background: #F4BF33;
border: none;
cursor: pointer;
cursor: hand;
}

dl.commentform dt input:hover
{
color: #F4BF33;
background: #353535;
}

.textline, .textmultiline
{
background-color: #dcdcdc;
border: 1px solid #505050;
color: #181818;
font: 1em "Segoe UI", "Trebuchet MS", Arial, Verdana, Helvetica, FreeSans, sans-serif;
padding: 5px;
width: 20em;
}

.textmultiline
{
width: 30em;
}

#subcontent-a
{
width: 260px;
margin: 0;
padding: 10px 20px;
background: #555;
box-shadow: #aaa -1px 1px 5px;
-moz-box-shadow: #aaa -1px 1px 5px;
-webkit-box-shadow: #aaa -1px 1px 5px;
}

#subcontent-a a:link, #subcontent-a a:visited
{
padding: 2px 5px;
color: #F4BF33;
text-decoration: none;
}

#subcontent-a a:hover
{
color: #464646;
background: #F4BF33;
border-radius: 3px;
-moz-border-radius: 3px;
-webkit-border-radius: 3px;
}

.searchbox {
background-color: #dcdcdc;
border: 1px solid #505050;
color: #181818;
font: 1em "Segoe UI", "Trebuchet MS", Arial, Verdana, Helvetica, FreeSans, sans-serif;
padding: 5px;
width: 15em;
}

.searchbox:focus, .textline:focus, .textmultiline:focus
{
border: 1px solid #F4BF33;
outline: none;
}

.searchbutton
{
font-size: 1.1em;
padding: 5px;
}

li.rss {
background: url(/themes/blogs/gold%20coast/images/rss.gif) no-repeat 40px 0px;
}

#footer
{
clear: both;
margin: 20px 0px 0px 0px;
padding: 20px 0px;
text-align: right;
border-top: 1px solid #aaa;
}

#footer img
{
display: none;
}

.Copyright
{
color: #505050;
margin: 0px 0px 20px 0px;
}

.entryview, dl.entrylist, .commentform, dl.commentform dt input, #subcontent-a
{
border-radius: 7px;
-moz-border-radius: 7px;
-webkit-border-radius: 7px;
}

td
{
    padding: 5px;
    background-color: #efefef;
}

.dp-highlighter
{
margin: 15px 0px 15px 0px;
}